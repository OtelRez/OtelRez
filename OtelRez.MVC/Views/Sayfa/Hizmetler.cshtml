@model Hizmetler

<!doctype html>
<html class="no-js" lang="zxx">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Hizmetlerimiz</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="manifest" href="site.webmanifest">
    <link rel="shortcut icon" type="image/x-icon" href="~/OtelTemp/assets/img/favicon.ico">

    <link rel="stylesheet" href="~/OtelTemp/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/gijgo.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/slicknav.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/animate.min.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/magnific-popup.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/fontawesome-all.min.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/themify-icons.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/themify-icons.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/slick.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/nice-select.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/style.css">
    <link rel="stylesheet" href="~/OtelTemp/assets/css/responsive.css">
</head>

<body>

    <!-- Preloader Start -->
    <div id="preloader-active">
        <div class="preloader d-flex align-items-center justify-content-center">
            <div class="preloader-inner position-relative">
                <div class="preloader-circle"></div>
                <div class="preloader-img pere-text">
                    <strong>Hotel</b>
                </div>
            </div>
        </div>
    </div>
    <!-- Preloader Start -->
    <main>

        <!-- slider Area Start-->
        <div class="slider-area">
            <div class="single-slider hero-overly slider-height2 d-flex align-items-center" data-background="/OtelTemp/assets/img/hero/servicespage_hero.jpg">
                <div class="container">
                    <div class="row ">
                        <div class="col-md-11 offset-xl-1 offset-lg-1 offset-md-1">
                            <div class="hero-caption">
                                <span>HİZMETLERİMİZ</span>
                                <h2>Hizmetler</h2>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- slider Area End-->

        <!-- Dining Start -->
        <div class="dining-area dining-padding-top">
            @await Component.InvokeAsync("Hizmetler")
        </div>  
        <!-- Dining End -->

        <br />
		<br />

        <!-- Gallery img Start-->
        <div class="gallery-area fix">
            <div class="container-fluid p-0">
                <div class="row">
                    <div class="col-md-12">
                        <div class="gallery-active owl-carousel">
                            <div class="gallery-img">
                                <a><img src="~/OtelTemp/assets/img/gallery/gallery1.jpg" alt=""></a>
                            </div>
                            <div class="gallery-img">
                                <a><img src="~/OtelTemp/assets/img/gallery/gallery2.jpg" alt=""></a>
                            </div>
                            <div class="gallery-img">
                                <a><img src="~/OtelTemp/assets/img/gallery/gallery3.jpg" alt=""></a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Gallery img End-->
    </main>

    <!-- JS here -->
    <!-- All JS Custom Plugins Link Here here -->
    <script src="~/OtelTemp/assets/js/vendor/modernizr-3.5.0.min.js"></script>

    <!-- Jquery, Popper, Bootstrap -->
    <script src="~/OtelTemp/assets/js/vendor/jquery-1.12.4.min.js"></script>
    <script src="~/OtelTemp/assets/js/popper.min.js"></script>
    <script src="~/OtelTemp/assets/js/bootstrap.min.js"></script>
    <!-- Jquery Mobile Menu -->
    <script src="~/OtelTemp/assets/js/jquery.slicknav.min.js"></script>

    <!-- Jquery Slick , Owl-Carousel Plugins -->
    <script src="~/OtelTemp/assets/js/owl.carousel.min.js"></script>
    <script src="~/OtelTemp/assets/js/slick.min.js"></script>
    <!-- Date Picker -->
    <script src="~/OtelTemp/assets/js/gijgo.min.js"></script>
    <!-- One Page, Animated-HeadLin -->
    <script src="~/OtelTemp/assets/js/wow.min.js"></script>
    <script src="~/OtelTemp/assets/js/animated.headline.js"></script>

    <!-- Scrollup, nice-select, sticky -->
    <script src="~/OtelTemp/assets/js/jquery.scrollUp.min.js"></script>
    <script src="~/OtelTemp/assets/js/jquery.nice-select.min.js"></script>
    <script src="~/OtelTemp/assets/js/jquery.sticky.js"></script>
    <script src="~/OtelTemp/assets/js/jquery.magnific-popup.js"></script>

    <!-- contact js -->
    <script src="~/OtelTemp/assets/js/contact.js"></script>
    <script src="~/OtelTemp/assets/js/jquery.form.js"></script>
    <script src="~/OtelTemp/assets/js/jquery.validate.min.js"></script>
    <script src="~/OtelTemp/assets/js/mail-script.js"></script>
    <script src="~/OtelTemp/assets/js/jquery.ajaxchimp.min.js"></script>

    <!-- Jquery Plugins, main Jquery -->
    <script src="~/OtelTemp/assets/js/plugins.js"></script>
    <script src="~/OtelTemp/assets/js/main.js"></script>

</body>
</html>